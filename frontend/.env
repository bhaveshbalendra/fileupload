# Environment Configuration
# Copy this file to .env and configure the values for your environment

# ================================
# API Configuration
# ================================
# Base URL for the backend API
VITE_API_BASE_URL=http://localhost:8000

# ================================
# Application Configuration
# ================================
# Application name and version
VITE_APP_NAME=FileUpload
VITE_APP_VERSION=1.0.0

# ================================
# Security Configuration
# ================================
# Secret key for Redux Persist encryption (optional)
VITE_REDUX_PERSIST_SECRET_KEY=my-secret-key-123

# ================================
# Feature Flags
# ================================
# Enable analytics tracking
VITE_ENABLE_ANALYTICS=true

# Enable debug mode (shows console logs)
VITE_ENABLE_DEBUG=true

# Enable development tools (for development only)
VITE_ENABLE_DEV_TOOLS=true